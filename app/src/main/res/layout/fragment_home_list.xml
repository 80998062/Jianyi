<?xml version="1.0" encoding="utf-8"?>

<com.sinyuk.jianyimaterial.widgets.MultiSwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
